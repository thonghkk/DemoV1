<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.coroutines_example">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />

    <application
        android:name=".notification.CheckSDK"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Coroutines_Example">
        <activity android:name=".page_recycle_view_type_2.PageRecycleViewBottomActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".page_recycle_view_type_1.PageRecycleViewActivity">

        </activity>
        <activity android:name=".animation.AnimationActivity" />
        <activity android:name=".get_video_on_drive.video.PlayVideoActivity" />
        <activity android:name=".get_video_on_drive.GetAllVideo"></activity>
        <activity android:name=".slide_image.SlideImageActivity" />
        <activity android:name=".notification.NotificationActivity" />
        <activity android:name=".CoroutineActivity" />
        <activity android:name=".recycle_view.MainActivity" />
        <activity android:name=".coutinescontext.DemoCoroutineScope" />
    </application>

</manifest>